{% comment %}
  Community Creation landing page section.
  Marketing/explainer page for gym owners — walks through the program and converts to subscribe.

  SHOPIFY ADMIN SETUP:
  1. In Shopify Admin → Online Store → Pages, create a new page called "Create a Community"
  2. In the page editor, change the "Theme template" dropdown from "Default page" to "community-creation"
  3. Save, then customize via Online Store → Customize → navigate to the page
{% endcomment %}

{% style %}
  #shopify-section-{{ section.id }} {
    --cc-hero-eyebrow:  {{ section.settings.hero_eyebrow_color }};
    --cc-hero-h1:       {{ section.settings.hero_headline1_color }};
    --cc-hero-h2:       {{ section.settings.hero_headline2_color }};
    --cc-hero-sub:      {{ section.settings.hero_subtext_color }};
    --cc-hero-bg:       {{ section.settings.hero_bg_color }};
    --cc-steps-bg:      {{ section.settings.steps_bg_color }};
    --cc-steps-label:   {{ section.settings.steps_label_color }};
    --cc-steps-num:     {{ section.settings.steps_number_color }};
    --cc-steps-title:   {{ section.settings.steps_title_color }};
    --cc-steps-body:    {{ section.settings.steps_body_color }};
    --cc-steps-ph-bg:   {{ section.settings.steps_placeholder_bg }};
    --cc-steps-ph-bdr:  {{ section.settings.steps_placeholder_border }};
    --cc-why-bg:        {{ section.settings.why_bg_color }};
    --cc-why-label:     {{ section.settings.why_label_color }};
    --cc-why-title:     {{ section.settings.why_section_title_color }};
    --cc-why-card-bg:   {{ section.settings.why_card_bg }};
    --cc-why-card-bdr:  {{ section.settings.why_card_border }};
    --cc-why-icon:      {{ section.settings.why_icon_color }};
    --cc-why-prop-title:{{ section.settings.why_prop_title_color }};
    --cc-why-prop-body: {{ section.settings.why_prop_body_color }};
    --cc-cta-bg:        {{ section.settings.cta_bg_color }};
    --cc-cta-title:     {{ section.settings.cta_title_color }};
  }
{% endstyle %}

<!-- ====================================================
     HERO
     ==================================================== -->
<div class="cc-hero full-width">
  <div class="cc-hero__overlay" aria-hidden="true"></div>
  <div class="cc-hero__grid"    aria-hidden="true"></div>

  <div class="cc-hero__content">
    {%- if section.settings.hero_eyebrow != blank -%}
      <p class="cc-hero__eyebrow fade-up" data-delay="1">
        {{ section.settings.hero_eyebrow }}
      </p>
    {%- endif -%}

    <h1 class="cc-hero__headline">
      {%- if section.settings.hero_headline_line1 != blank -%}
        <span class="cc-hero__hl1 fade-up" data-delay="2">
          {{- section.settings.hero_headline_line1 -}}
        </span>
      {%- endif -%}
      {%- if section.settings.hero_headline_line2 != blank -%}
        <span class="cc-hero__hl2 fade-up" data-delay="3">
          {{- section.settings.hero_headline_line2 -}}
        </span>
      {%- endif -%}
    </h1>

    {%- if section.settings.hero_subheadline != blank -%}
      <p class="cc-hero__sub fade-up" data-delay="4">
        {{ section.settings.hero_subheadline }}
      </p>
    {%- endif -%}

    {%- if section.settings.hero_cta_text != blank -%}
      <div class="cc-hero__cta fade-up" data-delay="5">
        <a href="{{ section.settings.hero_cta_url | default: '#' }}" class="btn btn--primary">
          {{ section.settings.hero_cta_text }}
        </a>
      </div>
    {%- endif -%}
  </div>
</div>

<!-- ====================================================
     HOW IT WORKS — 3 STEPS
     ==================================================== -->
<div class="cc-steps full-width">
  <div class="cc-steps__inner">
    {%- if section.settings.steps_eyebrow != blank -%}
      <p class="cc-steps__eyebrow fade-up" data-delay="1">
        {{ section.settings.steps_eyebrow }}
      </p>
    {%- endif -%}

    {%- if section.settings.steps_heading != blank -%}
      <h2 class="cc-steps__heading fade-up" data-delay="2">
        {{ section.settings.steps_heading }}
      </h2>
    {%- endif -%}

    <div class="cc-steps__grid">
      {%- for i in (1..3) -%}
        {%- assign step_num       = i -%}
        {%- assign title_key      = 'step' | append: i | append: '_title' -%}
        {%- assign desc_key       = 'step' | append: i | append: '_description' -%}
        {%- assign img_key        = 'step' | append: i | append: '_image' -%}
        {%- assign step_title     = section.settings[title_key] -%}
        {%- assign step_desc      = section.settings[desc_key] -%}
        {%- assign step_img       = section.settings[img_key] -%}

        <div class="cc-step fade-up" data-delay="{{ forloop.index | plus: 2 }}">
          <!-- Image / placeholder -->
          <div class="cc-step__img-wrap">
            {%- if step_img != blank -%}
              {{
                step_img
                | image_url: width: 600
                | image_tag:
                  class: 'cc-step__img',
                  loading: 'lazy',
                  alt: step_title
              }}
            {%- else -%}
              <div class="cc-step__placeholder" aria-hidden="true">
                <span class="cc-step__placeholder-label">{{ section.settings.placeholder_label }}</span>
              </div>
            {%- endif -%}
          </div>

          <div class="cc-step__body">
            <span class="cc-step__num" aria-hidden="true">0{{ step_num }}</span>
            <h3 class="cc-step__title">{{ step_title }}</h3>
            <p class="cc-step__desc">{{ step_desc }}</p>
          </div>
        </div>
      {%- endfor -%}
    </div>
  </div>
</div>

<!-- ====================================================
     WHY IT WORKS — VALUE PROPS
     ==================================================== -->
<div class="cc-why full-width">
  <div class="cc-why__inner">
    {%- if section.settings.why_eyebrow != blank -%}
      <p class="cc-why__eyebrow fade-up" data-delay="1">
        {{ section.settings.why_eyebrow }}
      </p>
    {%- endif -%}

    {%- if section.settings.why_heading != blank -%}
      <h2 class="cc-why__heading fade-up" data-delay="2">
        {{ section.settings.why_heading }}
      </h2>
    {%- endif -%}

    <div class="cc-why__grid">
      {%- for i in (1..4) -%}
        {%- assign prop_title_key = 'prop' | append: i | append: '_title' -%}
        {%- assign prop_body_key  = 'prop' | append: i | append: '_body' -%}
        {%- assign prop_icon_key  = 'prop' | append: i | append: '_icon' -%}
        {%- assign prop_title = section.settings[prop_title_key] -%}
        {%- assign prop_body  = section.settings[prop_body_key] -%}
        {%- assign prop_icon  = section.settings[prop_icon_key] -%}

        {%- if prop_title != blank -%}
          <div class="cc-prop fade-up" data-delay="{{ forloop.index | plus: 2 }}">
            {%- if prop_icon != blank -%}
              <span class="cc-prop__icon" aria-hidden="true">{{ prop_icon }}</span>
            {%- endif -%}
            <h3 class="cc-prop__title">{{ prop_title }}</h3>
            {%- if prop_body != blank -%}
              <p class="cc-prop__body">{{ prop_body }}</p>
            {%- endif -%}
          </div>
        {%- endif -%}
      {%- endfor -%}
    </div>
  </div>
</div>

<!-- ====================================================
     FINAL CTA
     ==================================================== -->
<div class="cc-cta full-width">
  <div class="cc-cta__grid-bg" aria-hidden="true"></div>

  <div class="cc-cta__inner">
    {%- if section.settings.cta_eyebrow != blank -%}
      <p class="cc-cta__eyebrow fade-up" data-delay="1">
        {{ section.settings.cta_eyebrow }}
      </p>
    {%- endif -%}

    {%- if section.settings.cta_heading != blank -%}
      <h2 class="cc-cta__heading fade-up" data-delay="2">
        {{ section.settings.cta_heading }}
      </h2>
    {%- endif -%}

    {%- if section.settings.cta_subtext != blank -%}
      <p class="cc-cta__subtext fade-up" data-delay="3">
        {{ section.settings.cta_subtext }}
      </p>
    {%- endif -%}

    {%- if section.settings.cta_btn_text != blank -%}
      <div class="fade-up" data-delay="4">
        <a href="{{ section.settings.cta_btn_url | default: '#' }}" class="btn btn--primary">
          {{ section.settings.cta_btn_text }}
        </a>
      </div>
    {%- endif -%}
  </div>
</div>

{% stylesheet %}
/* ============================================================
   SHARED / PAGE LAYOUT
   ============================================================ */
.cc-hero,
.cc-steps,
.cc-why,
.cc-cta {
  width: 100%;
}

/* ============================================================
   HERO
   ============================================================ */
.cc-hero {
  position: relative;
  background-color: var(--cc-hero-bg, #111009);
  min-height: 72vh;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.cc-hero__overlay {
  position: absolute;
  inset: 0;
  z-index: 1;
  background:
    radial-gradient(ellipse at 10% 60%, rgba(74, 92, 42, 0.30) 0%, transparent 55%),
    radial-gradient(ellipse at 90% 10%, rgba(212, 98, 42, 0.14) 0%, transparent 48%),
    linear-gradient(to bottom, rgba(17, 16, 9, 0.60) 0%, rgba(17, 16, 9, 0.85) 100%);
}

.cc-hero__grid {
  position: absolute;
  inset: 0;
  z-index: 2;
  background-image:
    linear-gradient(rgba(74, 92, 42, 0.07) 1px, transparent 1px),
    linear-gradient(90deg, rgba(74, 92, 42, 0.07) 1px, transparent 1px);
  background-size: 56px 56px;
}

.cc-hero__content {
  position: relative;
  z-index: 3;
  padding: clamp(80px, 12vh, 160px) clamp(24px, 6vw, 100px);
  max-width: 1000px;
  width: 100%;
}

.cc-hero__eyebrow {
  font-family: var(--font-label);
  font-size: 11px;
  font-weight: 500;
  letter-spacing: 5px;
  text-transform: uppercase;
  color: var(--cc-hero-eyebrow, #d4622a);
  margin-bottom: 1.25rem;
}

.cc-hero__headline {
  font-family: var(--font-heading);
  font-size: clamp(56px, 8.5vw, 120px);
  line-height: 0.92;
  letter-spacing: -1px;
  margin-bottom: 1.75rem;
  display: flex;
  flex-direction: column;
  gap: 0.06em;
}

.cc-hero__hl1 {
  color: var(--cc-hero-h1, #faf8f4);
  display: block;
}

.cc-hero__hl2 {
  color: var(--cc-hero-h2, #c8a96e);
  display: block;
}

.cc-hero__sub {
  font-family: var(--font-body);
  font-size: clamp(17px, 2.2vw, 22px);
  font-weight: 300;
  color: var(--cc-hero-sub, #e8d5b0);
  max-width: 560px;
  line-height: 1.65;
  margin-bottom: 2.5rem;
}

.cc-hero__cta {
  display: flex;
  gap: 1rem;
  flex-wrap: wrap;
}

/* ============================================================
   HOW IT WORKS — STEPS
   ============================================================ */
.cc-steps {
  background-color: var(--cc-steps-bg, #1e1e1a);
  padding: 100px clamp(24px, 6vw, 100px);
}

.cc-steps__inner {
  max-width: 1200px;
  margin: 0 auto;
}

.cc-steps__eyebrow {
  font-family: var(--font-label);
  font-size: 11px;
  font-weight: 500;
  letter-spacing: 5px;
  text-transform: uppercase;
  color: var(--cc-steps-label, #d4622a);
  margin-bottom: 1rem;
  text-align: center;
}

.cc-steps__heading {
  font-family: var(--font-heading);
  font-size: clamp(32px, 4.5vw, 60px);
  color: var(--cc-steps-title, #faf8f4);
  line-height: 1;
  text-align: center;
  margin-bottom: 3.5rem;
  text-transform: uppercase;
}

.cc-steps__grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 32px;
}

.cc-step {
  display: flex;
  flex-direction: column;
  gap: 24px;
}

/* Placeholder image slot */
.cc-step__img-wrap {
  width: 100%;
  aspect-ratio: 4 / 3;
  overflow: hidden;
  border: 1px solid var(--cc-steps-ph-bdr, #2d3a18);
}

.cc-step__img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.cc-step__placeholder {
  width: 100%;
  height: 100%;
  background-color: var(--cc-steps-ph-bg, #18180e);
  display: flex;
  align-items: center;
  justify-content: center;
}

.cc-step__placeholder-label {
  font-family: var(--font-label);
  font-size: 10px;
  letter-spacing: 3px;
  text-transform: uppercase;
  color: #3a3a30;
}

.cc-step__body {
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.cc-step__num {
  font-family: var(--font-heading);
  font-size: 52px;
  line-height: 1;
  color: var(--cc-steps-num, #2d3a18);
  letter-spacing: -2px;
}

.cc-step__title {
  font-family: var(--font-heading);
  font-size: clamp(22px, 2.5vw, 30px);
  color: var(--cc-steps-title, #faf8f4);
  text-transform: uppercase;
  line-height: 1.1;
}

.cc-step__desc {
  font-family: var(--font-body);
  font-size: 15px;
  font-weight: 300;
  color: var(--cc-steps-body, #e8d5b0);
  line-height: 1.7;
}

/* ============================================================
   WHY IT WORKS — VALUE PROPS
   ============================================================ */
.cc-why {
  background-color: var(--cc-why-bg, #111009);
  padding: 100px clamp(24px, 6vw, 100px);
}

.cc-why__inner {
  max-width: 1200px;
  margin: 0 auto;
}

.cc-why__eyebrow {
  font-family: var(--font-label);
  font-size: 11px;
  font-weight: 500;
  letter-spacing: 5px;
  text-transform: uppercase;
  color: var(--cc-why-label, #d4622a);
  margin-bottom: 1rem;
  text-align: center;
}

.cc-why__heading {
  font-family: var(--font-heading);
  font-size: clamp(32px, 4.5vw, 60px);
  color: var(--cc-why-title, #faf8f4);
  line-height: 1;
  text-align: center;
  margin-bottom: 3.5rem;
  text-transform: uppercase;
}

.cc-why__grid {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 24px;
}

.cc-prop {
  background-color: var(--cc-why-card-bg, #1e1e1a);
  border: 1px solid var(--cc-why-card-bdr, #2d3a18);
  padding: 36px 32px;
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.cc-prop__icon {
  font-size: 28px;
  color: var(--cc-why-icon, #c8a96e);
  line-height: 1;
}

.cc-prop__title {
  font-family: var(--font-heading);
  font-size: clamp(18px, 1.8vw, 22px);
  color: var(--cc-why-prop-title, #faf8f4);
  text-transform: uppercase;
  line-height: 1.1;
}

.cc-prop__body {
  font-family: var(--font-body);
  font-size: 15px;
  font-weight: 300;
  color: var(--cc-why-prop-body, #e8d5b0);
  line-height: 1.7;
}

/* ============================================================
   FINAL CTA
   ============================================================ */
.cc-cta {
  position: relative;
  background-color: var(--cc-cta-bg, #2d3a18);
  padding: 100px clamp(24px, 6vw, 100px);
  overflow: hidden;
}

.cc-cta__grid-bg {
  position: absolute;
  inset: 0;
  background-image:
    linear-gradient(rgba(200, 169, 110, 0.05) 1px, transparent 1px),
    linear-gradient(90deg, rgba(200, 169, 110, 0.05) 1px, transparent 1px);
  background-size: 48px 48px;
  pointer-events: none;
}

.cc-cta__inner {
  position: relative;
  z-index: 1;
  max-width: 800px;
  margin: 0 auto;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 24px;
}

.cc-cta__eyebrow {
  font-family: var(--font-label);
  font-size: 11px;
  font-weight: 500;
  letter-spacing: 5px;
  text-transform: uppercase;
  color: var(--cc-hero-eyebrow, #d4622a);
}

.cc-cta__heading {
  font-family: var(--font-heading);
  font-size: clamp(36px, 5.5vw, 72px);
  color: var(--cc-cta-title, #faf8f4);
  line-height: 1;
  text-transform: uppercase;
}

.cc-cta__subtext {
  font-family: var(--font-body);
  font-size: 17px;
  font-weight: 300;
  color: var(--cc-steps-body, #e8d5b0);
  line-height: 1.65;
  max-width: 520px;
}

/* ============================================================
   RESPONSIVE
   ============================================================ */
@media (max-width: 900px) {
  .cc-steps__grid {
    grid-template-columns: 1fr;
    gap: 48px;
    max-width: 520px;
    margin-left: auto;
    margin-right: auto;
  }

  .cc-step__img-wrap {
    aspect-ratio: 16 / 9;
  }
}

@media (max-width: 768px) {
  .cc-hero { min-height: 80svh; }

  .cc-hero__headline { font-size: clamp(48px, 13vw, 80px); }

  .cc-steps,
  .cc-why,
  .cc-cta { padding: 72px clamp(20px, 5vw, 48px); }

  .cc-why__grid {
    grid-template-columns: 1fr;
  }

  .cc-prop { padding: 28px 24px; }
}
{% endstylesheet %}

